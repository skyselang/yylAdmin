{
  "name": "skyselang/yyladmin",
  "homepage": "https://gitee.com/skyselang/yyladmin",
  "description": "Minimalist background management system based on ThinkPHP and Vue",
  "keywords": [
    "vue",
    "php",
    "element-plus",
    "thinkphp",
    "admin",
    "yyladmin"
  ],
  "authors": [
    {
      "name": "skyselang",
      "email": "215817969@qq.com"
    }
  ],
  "license": "Apache-2.0",
  "type": "project",
  "scripts": {
    "post-autoload-dump": [
      "@php think service:discover",
      "@php think vendor:publish"
    ]
  },
  "require": {
    "php": ">=8.2",
    "topthink/framework": "^8.1",
    "topthink/think-orm": "^4.0",
    "topthink/think-multi-app": "^1.0",
    "topthink/think-throttle": "^2.0",
    "topthink/think-filesystem": "^3.0",
    "topthink/think-queue": "^3.0",
    "firebase/php-jwt": "^6.1",
    "overtrue/pinyin": "^5.3",
    "overtrue/easy-sms": "^3.0",
    "phpmailer/phpmailer": "^6.9",
    "fastknife/ajcaptcha": "^1.2",
    "chinayin/ip2region": "^2.0",
    "hg/apidoc": "5.3.0",
    "qiniu/php-sdk": "^7.11",
    "aliyuncs/oss-sdk-php": "^2.7",
    "qcloud/cos-sdk-v5": "^2.6",
    "alapi/upyun-sdk": "^4.0",
    "aws/aws-sdk-php": "^3.333",
    "guzzlehttp/guzzle": "^7.0",
    "openspout/openspout": "^4.28",
    "workerman/workerman": "^5.0",
    "workerman/crontab": "^1.0"
  },
  "require-dev": {
    "symfony/var-dumper": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "app\\": "app"
    },
    "psr-0": {
      "": "extend/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "allow-plugins": {}
  }
}
